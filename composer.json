{
	"name": "dmzx/newestmembers",
	"type": "phpbb-extension",
	"description": "This extension shows the newest members instead of only one.",
	"homepage": "http://www.dmzx-web.net",
	"version": "1.0.2",
	"time": "2017-01-21",
	"keywords": [
		"phpbb",
		"extension",
		"Newest members"
	],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "http://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"phpbb/phpbb": "3.1.*"
	},
	"extra": {
		"display-name": "Newest members",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0,<3.3"
		},
		"version-check": {
			"host": "www.phpbb.com",
			"directory": "/customise/db/extension/newest_members",
			"filename": "version_check"
		}
	}
}